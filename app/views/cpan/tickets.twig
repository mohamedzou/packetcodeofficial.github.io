{% include 'header.twig' %}
      <main class="inner-wrap">
        <div class="container-fluid">
  <div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-4"><p class="breadcrumbs">You are currently on: <strong>Tickets</strong></p></div>
    <div class="col-sm-4"></div>
  </div>
  <div class="row">
    <div class="col-sm-12">
      <button id="fund-history" class="btn btn-ticket">Check History</button>
    </div>
  </div>
  <div id="ticket-form" class="create-ticket-wrap">
      {% if error %}
        <div class="alert alert-dismissible alert-danger {% if site['rtl'] %} rtl-alert {% endif %}">
          <button type="button" class="close" data-dismiss="alert">×</button>
          {{ errorText }}
        </div>
      {% elseif success %}
        <div class="alert alert-dismissible alert-success {% if site['rtl'] %} rtl-alert {% endif %}">
          <button type="button" class="close" data-dismiss="alert">×</button>
          {{ successText }}
        </div>
      {% endif %}
     <form method="post" action="tickets">
            <div class="form-group">
              <label for="subject" class="control-label">{{ lang['tickets.subject'] }}</label>
                <input class="form-control" id="subject" name="subject">
                {#  {% for subjects in orders %}#}
                {#   <option value"{{ subjects['subject'] }}">{{ subjects['subject'] }}</option>#}
                {#  {% endfor %}#}
                {#</select>  #}
            </div>
            <div class="form-group">
              <label for="message" class="control-label">{{ lang['tickets.message'] }}</label>
              <textarea class="form-control" rows="7" id="message" name="message">{{ data['message'] }}</textarea>
            </div>
            <button type="submit" class="btn btn-primary">{#{{ lang['tickets.button'] }}#}Create Ticket</button>
          </form>
  </div>
  <div id="ticket-table" class="ticket-history-wrap hidden">
            <form action="/en/tickets" method="get" id="history-search">
      <div class="input-group search-bar">
        <input type="text" class="form-control" value="" name="search" placeholder="Search">
        <span class="input-group-btn">
          <button type="submit" class="btn btn-default"><i class="far fa-search" aria-hidden="true"></i></button>
        </span>
      </div>
    </form>
        <div class="table-responsive">
      <table class="table custom-table mob-table ">
        <thead>
          <tr>
            <th>ID</th>
            <th>Subject</th>
            <th class="nowrap">Last update</th>
            <th class="text-center">Status</th>
          </tr>
        </thead>
        <tbody>
            {% for ticket in ticketList %}
                <tr class="table-cont">
                  <td>{{ ticket['ticket_id'] }}</td>
                  {% if ticket["support_new"] == 2 %}
                    <td><a href="tickets/{{ ticket['ticket_id'] }}"><strong>{{ ticket['subject'] }}</strong></a></td>
                  {% else %}
                    <td><a href="tickets/{{ ticket['ticket_id'] }}">{{ ticket['subject'] }}</a></td>
                  {% endif %}
                  <td>{{ ticket['status'] }}</td>
                  <td><span class="nowrap">{{ ticket['lastupdate_time'] }}</span></td>
                </tr>
              {% endfor %}
          {#          <tr class="table-cont">#}
          {#  <td data-head="ID">123683</td>#}
          {#              <td data-head="Subject"><a class="table-link" href="/en/viewticket/123683"><span><i class="far fa-envelope"></i>الخدمات</span></a></td>#}
          {#              <td data-head="Last update"><span class="nowrap">2021-10-15 01:39:27</span></td>#}
          {#  <td data-head="Status"><div class="status-badge badge-complete"><span>Answered</span></div></td>#}
          {#</tr>#}
                  </tbody>
      </table>
    </div>
          </div>
</div>
      </main>
      {% include 'footer.twig' %}
    </div>


    
            
