{% include 'header.twig' %}
      <main class="inner-wrap">
        <div class="container-fluid">
  <div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-4"><p class="breadcrumbs">You are currently on: <strong>Add funds</strong></p></div>
    <div class="col-sm-4"></div>
  </div>
  <div class="row">
    <div class="col-md-12">
      <div class="noti-bar noti-success">
        <div class="noti-icon"></div>
                <p>Important: To Activate Visa/MasterCard Payments please contact us here  [ <a href="{{ site['url'] }}/tickets" target="_blank">Ticket</a> ]<br></p><p>Important: To Activate PayPal Payments please contact us here  [ <a href="{{ site['url'] }}/tickets" target="_blank">Ticket</a> ]<br></p><p>*If you Wanted to Pay via any other Method please contact  us via tickets</p>
               
      </div>
    </div>
  </div>
  <div class="row">
        <div class="col-md-7">
      <ul class="nav nav-tabs" role="tablist">
        <li class="active"><a href="#add-funds" aria-controls="add-funds" role="tab" data-toggle="tab">Add Funds</a></li>
        <li><a href="#history" aria-controls="history" role="tab" data-toggle="tab">History</a></li>
      </ul>
      <div class="well">
            {% if error %}
                                      <div class="alert alert-dismissible alert-danger">
                                        <button type="button" class="close" data-dismiss="alert">&times;</button>
                                        {{ errorText }}
                                      </div>
                                    {% endif %}
                                    {% if success %}
                                      <div class="alert alert-dismissible alert-success">
                                        <button type="button" class="close" data-dismiss="alert">&times;</button>
                                        {{ successText }}
                                      </div>
                                    {% endif %}
        <div class="tab-content">
          <div role="tabpanel" class="tab-pane active" id="add-funds">
            <form  method="post" action="">
                {% if paymentsList %}
                                   
                                    
                              			<br>
                                          <form method="post" action="addfunds">
                            
                                            <div class="form-group">
                                              <label for="method" class="control-label">{{ lang['addfunds.method'] }}</label>
                                              <select class="form-control" id="payment_method" name="payment_type">
               {% for payment in paymentsList %}
                        {% if payment['id'] != 20 %}
                        <option value="{{ payment['id'] }}" {% if data['payment_type'] == payment['id'] %} selected {% endif %} >{{ payment['method_name'] }}</option>
        {% endif %}
                        {% endfor %}
                                              </select>
                                            </div>
                            
                                              <div class="form-group">
                                                  <label for="method" class="control-label">{{ lang['addfunds.amount'] }}</label>
                                                  <input class="form-control" name="payment_amount" value="{{ data['payment_amount'] }}">
                                              </div>
                            
                                            <button type="submit" class="btn btn-block btn-primary" style="padding:0px;">{{ lang['addfunds.button'] }}</button>
                                          </form>
                                                                                                       
                                                                                                     
                                    {% endif %}
            </form>
          </div>
          <div role="tabpanel" class="tab-pane" id="history">
              
              
																		  <table class="table" style="color:black;">
										 <thead>
											<tr>
											   <th style="color:black;">ID</th>
											   <th style="color:black;">Date</th>
											   <th>Method</th>
											   <th>Amount</th>
											</tr>
										 </thead>
										 <tbody>
										     {% for transaction in transactions %}
										 											<tr>
											   <td>{{ transaction['payment_id'] }}</td>
											   <td><span class="nowrap">{{ transaction['payment_create_date'] }}</span></td>
											   <td>â‚¹ Paytm / UPI / Cards/Netbanking{{ transaction['method_name'] }}</td>
											   <td><strong>{{ transaction['payment_amount'] }}</strong></td>
											</tr>
											{% endfor %}
										 </tbody>
										 
									  </table>
									  
              
                      </div>
        </div>
